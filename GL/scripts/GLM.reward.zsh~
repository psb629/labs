#!/bin/zsh

dir_fmri=/mnt/sdb2/GL/fmri_data

subj='GL03'
dir_preproc=$dir_fmri/preproc_data/$subj
dir_output=$dir_fmri/stats/GLM.reward/$subj

3dDeconvolve -input \
	$dir_preproc/pb05.$subj.r02.scale+orig \
	$dir_preproc/pb05.$subj.r03.scale+orig \
	$dir_preproc/pb05.$subj.r04.scale+orig \
	$dir_preproc/pb05.$subj.r05.scale+orig \
	-mask $dir_preproc/full_mask.$subj+orig \
	-censor $dir_preproc/censor_${subj}_combined_2.1D \
	-polort A -float -local_times \
	-num_stimts 7 \
	-num_glt 1 -stim_times_AM2 1 /GL03_Rew.txt 'BLOCK(1,1)' -stim_label 1 Rew \
	-stim_file 2 'motion_demean.GL03.r02_07.1D[0]' -stim_base 2 -stim_label 2 roll \
	-stim_file 3 'motion_demean.GL03.r02_07.1D[1]' -stim_base 3 -stim_label 3 pitch \
	-stim_file 4 'motion_demean.GL03.r02_07.1D[2]' -stim_base 4 -stim_label 4 yaw \
	-stim_file 5 'motion_demean.GL03.r02_07.1D[3]' -stim_base 5 -stim_label 5 dS \
	-stim_file 6 'motion_demean.GL03.r02_07.1D[4]' -stim_base 6 -stim_label 6 dL \
	-stim_file 7 'motion_demean.GL03.r02_07.1D[5]' -stim_base 7 -stim_label 7 dP \
	-gltsym 'SYM: Rew' -glt_label 1 Rew \
	-fout -tout \
	-x1D X.xmat.1D \
	-xjpeg X.jpg \
	-x1D_uncensored X.nocensor.xmat.1D \
	-bucket /Users/sskim/Documents/Research/AFNI/GL/data/GL03/stats/statRew.GL03

